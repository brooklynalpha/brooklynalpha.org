RewriteEngine On
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]

RewriteRule ^([A-Za-z0-9]+)$ /2012/$1.html [L]

ErrorDocument 400 /2012/_error.html
ErrorDocument 401 /2012/_error.html
ErrorDocument 402 /2012/_error.html
ErrorDocument 403 /2012/_error.html
ErrorDocument 404 /2012/_error.html
